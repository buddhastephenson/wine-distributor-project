{
  "name": "aoc-wines-distributor",
  "version": "1.0.0",
  "description": "Wine distribution management system for AOC Wines",
  "private": true,
  "proxy": "http://localhost:3002",
  "dependencies": {
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.13.4",
    "bcryptjs": "^3.0.3",
    "body-parser": "^2.2.2",
    "cors": "^2.8.6",
    "date-fns": "^4.1.0",
    "express": "^5.2.1",
    "lucide-react": "^0.263.1",
    "mongoose": "^9.1.5",
    "multer": "^2.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.4.0",
    "react-router-dom": "^7.13.0",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node dist/server/server/app.js",
    "dev": "concurrently \"react-scripts start\" \"PORT=3002 TS_NODE_TRANSPILE_ONLY=true nodemon src/server/app.ts\"",
    "build:server": "tsc -p src/server",
    "start:server": "node dist/server/server/app.js",
    "server": "TS_NODE_TRANSPILE_ONLY=true nodemon src/server/app.ts",
    "client": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.2.2",
    "@types/nodemailer": "^7.0.9",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/react-dropzone": "^4.2.2",
    "@types/uuid": "^10.0.0",
    "@types/xlsx": "^0.0.35",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "concurrently": "^9.2.1",
    "eslint": "^10.0.0",
    "nodemailer": "^8.0.1",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.1",
    "react-scripts": "5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}